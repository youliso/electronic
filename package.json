{
  "name": "ym-electron",
  "version": "2.0.0",
  "description": "ym electron",
  "scripts": {
    "build": "rollup -c rollup.config.js && node build.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "fix:prettier": "prettier \"./src\" --write",
    "test:prettier": "prettier \"./src\" --list-different",
    "test": " npx electron ./test/main.js"
  },
  "keywords": [
    "electron"
  ],
  "author": "youliso",
  "repository": {
    "type": "git",
    "url": "https://github.com/youliso/electron-modules"
  },
  "engines": {
    "electron": ">=13",
    "node": ">=16.14.0"
  },
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "electron": "^19.0.4",
    "electron-updater": "^5.0.1",
    "prettier": "^2.7.1",
    "rollup": "^2.75.6",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  }
}
