{
  "name": "@youliso/electronic",
  "version": "6.0.5",
  "description": "electron modules",
  "scripts": {
    "build": "tsup && node build.mjs",
    "fix:prettier": "prettier \"./src\" --write --config .prettierrc",
    "test:prettier": "prettier \"./src\" --list-different --config .prettierrc"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "types.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./types": {
      "import": "./types.d.ts",
      "require": "./types.d.ts"
    },
    "./main": {
      "import": "./main.mjs",
      "require": "./main.js"
    },
    "./render": {
      "import": "./render.mjs",
      "require": "./render.js"
    },
    "./preload": {
      "import": "./preload.mjs",
      "require": "./preload.js"
    }
  },
  "keywords": [
    "electron",
    "multiple windows"
  ],
  "author": "youliso <youliso@hotmail.com> (https://github.com/youliso)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/youliso/electronic"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "electron": ">=32",
    "node": ">=22"
  },
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "@types/node": "^22.15.32",
    "electron": "^35.5.1",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
